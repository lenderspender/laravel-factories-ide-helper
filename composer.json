{
    "name": "lenderspender/laravel-factories-ide-helper",
    "description": "Generate IDE helper file for Laravel factories",
    "type": "library",
    "license": "proprietary",
    "authors": [
        {
            "name": "Bert van Hoekelen",
            "email": "bert@magwel.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "LenderSpender\\LaravelFactoriesIdeHelper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LenderSpender\\LaravelFactoriesIdeHelper\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4 | ^8.0",
        "barryvdh/laravel-ide-helper": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.12.64",
        "orchestra/testbench": "^3.7|^4.0|^5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LenderSpender\\LaravelFactoriesIdeHelper\\LaravelFactoriesIdeHelperServiceProvider"
            ]
        }
    }
}
